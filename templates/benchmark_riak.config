#jinja2:variable_start_string:'(<' , variable_end_string:'>)'
{mode, {rate, (< benchmark_suite_rate >) }}.

{duration, (< benchmark_suite_duration >) }.

{concurrent, (< benchmark_suite_concurrent >) }.

{driver, basho_bench_driver_riakc_pb}.

{key_generator, {int_to_bin, {uniform_int, 10000}}}.

{value_generator, {fixed_bin, 10000}}.

{% set adapter = 'ansible_' + riak_iface  %}

{riakc_pb_ips, [{% for server in groups[benchmark_cluster_group] %}{ (< hostvars[server][adapter].ipv4.address|replace(".",",") >) }{% if not loop.last %},{% endif %}{% endfor %}]}.

{riakc_pb_replies, 1}.

{operations, {get, 1} }.
